{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": "./js",
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"sourceType": "module",
	"plugins": ["plugins/markdown"],
	"opts": {
		"template": "node_modules/docdash",
		"encoding": "utf8",
		"destination": "./P8-Openclassrooms.wiki/docs/",
		"recurse": true,
		"verbose": true
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}
